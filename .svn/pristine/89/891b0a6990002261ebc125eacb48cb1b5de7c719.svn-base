<?xml version="1.0" encoding="UTF-8"?>
<name-definition id="$nameId" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="$schema">
  
  #if(!$model.getDomainTypeNameModelSet().getDomainTypeNamingModels().isEmpty())
  <domain-type-name-set>
  	#foreach($domainTypeNaming in $model.getDomainTypeNameModelSet().getDomainTypeNamingModels())
    <domain-type-name domain-type-ref="$domainTypeNaming.getDomainTypeModelReference()" table-name="$domainTypeNaming.getTableName()">
      #foreach($propertyNaming in $domainTypeNaming.getPropertyNamingModels())
      <property-name property-ref="$propertyNaming.getPropertyReference()" column-name="$propertyNaming.getColumnName()" />
      #end
  </domain-type-name>
    #end
</domain-type-name-set>
  #end
  
  #if(!$model.getRelationNameModelSet().getRelationNamingModels().isEmpty())
  <relation-name-set>
  
  	#foreach($relationNaming in $model.getRelationNameModelSet().getRelationNamingModels())
    <relation-name relation-ref="$relationNaming.getRelationModelReference()" table-name="$relationNaming.getTableName()">
    	
    	#foreach($foreignKey in $relationNaming.getPropertyNamingModels())
      	<foreign-key-name property-ref="$foreignKey.getPropertyReference()" column-name="$foreignKey.getColumnName()" />
      	#end
    </relation-name>
    #end
    
  </relation-name-set>
#end
</name-definition>