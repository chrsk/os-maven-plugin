package org.opensaga.plugin.builder.meta.parser;

/**
 * Indicates that the XML parser failed. This can be due to inconsistent and not
 * well formed XML files or not matching access logic. Maybe the underlying
 * schema changed.
 * 
 * @author cklewes
 */
public class XmlParserFailureException
    extends Exception
{

    private static final long serialVersionUID = 270809275163538397L;


    public XmlParserFailureException()
    {
    }


    public XmlParserFailureException(String message)
    {
        super(message);
    }


    public XmlParserFailureException(Throwable cause)
    {
        super(cause);
    }


    public XmlParserFailureException(String message, Throwable cause)
    {
        super(message, cause);
    }

}
