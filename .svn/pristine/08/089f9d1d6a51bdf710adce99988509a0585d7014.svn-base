package org.opensaga.plugin.release;

import java.util.Map;

public class ReleaseRequest
{

    private String version;
    
    private boolean integrationSupport;

    private String integrationServer;

    private String integrationServerLogin;

    private String integrationServerPassword;
    
    private String integrationServerSuperUserPassword;
    
    private String integrationServerTomcatName;

    private String integrationServerTomcatPath;
    
    private boolean ftpSupport;

    private String ftpServer;

    private String ftpServerLogin;

    private String ftpServerPort;

    private String ftpServerPath;

    private String ftpServerPassword;

    private Map<String, String> properties;

    private Map<String, String> promptableProperties;
    
    private boolean usedMavenGoals;

    private String goals;


    public String getVersion()
    {
        return version;
    }


    public ReleaseRequest setVersion(String version)
    {
        this.version = version;
        return this;
    }


    public String getIntegrationServer()
    {
        return integrationServer;
    }


    public ReleaseRequest setIntegrationServer(String integrationServer)
    {
        this.integrationServer = integrationServer;
        return this;
    }


    public String getIntegrationServerLogin()
    {
        return integrationServerLogin;

    }


    public ReleaseRequest setIntegrationServerLogin(String integrationServerLogin)
    {
        this.integrationServerLogin = integrationServerLogin;
        return this;
    }


    public String getIntegrationServerPassword()
    {
        return integrationServerPassword;
    }


    public ReleaseRequest setIntegrationServerPassword(String integrationServerPassword)
    {
        this.integrationServerPassword = integrationServerPassword;
        return this;
    }


    public String getFtpServer()
    {
        return ftpServer;
    }


    public ReleaseRequest setFtpServer(String ftpServer)
    {
        this.ftpServer = ftpServer;
        return this;
    }


    public String getFtpServerLogin()
    {
        return ftpServerLogin;
    }


    public ReleaseRequest setFtpServerLogin(String ftpServerLogin)
    {
        this.ftpServerLogin = ftpServerLogin;
        return this;
    }


    public String getFtpServerPort()
    {
        return ftpServerPort;
    }


    public ReleaseRequest setFtpServerPort(String ftpServerPort)
    {
        this.ftpServerPort = ftpServerPort;
        return this;
    }


    public String getFtpServerPassword()
    {
        return ftpServerPassword;
    }


    public ReleaseRequest setFtpServerPassword(String ftpServerPassword)
    {
        this.ftpServerPassword = ftpServerPassword;
        return this;
    }
    
    public String getIntegrationServerSuperUserPassword()
    {
        return this.integrationServerSuperUserPassword;
    }

    public ReleaseRequest setIntegrationServerSuperUserPassword(String integrationServerSuperUserPassword)
    {
        this.integrationServerSuperUserPassword = integrationServerSuperUserPassword;
        return this;
    }

    public boolean isIntegrationSupport()
    {
        return integrationSupport;
    }


    public ReleaseRequest setIntegrationSupport(boolean integrationSupport)
    {
        this.integrationSupport = integrationSupport;
        return this;
    }


    public boolean isFtpSupport()
    {
        return ftpSupport;
    }


    public ReleaseRequest setFtpSupport(boolean ftpSupport)
    {
        this.ftpSupport = ftpSupport;
        return this;
    }


    public boolean isUsedMavenGoals()
    {
        return usedMavenGoals;
    }


    public void setUsedMavenGoals(boolean usedMavenGoals)
    {
        this.usedMavenGoals = usedMavenGoals;
    }


    public ReleaseRequest setGoals(String goals)
    {
        this.goals = goals;
        return this;
    }


    public void addProperty(String key, String value)
    {
        this.properties.put(key, value);
    }


    public void addPromptableProperty(String key, String value)
    {
        this.promptableProperties.put(key, value);
    }


    public ReleaseRequest setProperties(Map<String, String> properties)
    {
        this.properties = properties;
        return this;
    }


    public Map<String, String> getProperties()
    {
        return properties;
    }


    public ReleaseRequest setPromptableProperties(Map<String, String> promptableProperties)
    {
        this.promptableProperties = promptableProperties;
        return this;
    }


    public Map<String, String> getPromptableProperties()
    {
        return promptableProperties;
    }


    public String getGoals()
    {
        return goals;
    }


    public ReleaseRequest setFtpServerPath(String ftpServerPath)
    {
        this.ftpServerPath = ftpServerPath;
        return this;
    }
    
    public String getFtpServerPath()
    {
        return ftpServerPath;
    }


    public String getIntegrationServerTomcatName()
    {
        return integrationServerTomcatName;
    }


    public ReleaseRequest setIntegrationServerTomcatName(String integrationServerTomcatName)
    {
        this.integrationServerTomcatName = integrationServerTomcatName;
        return this;
    }

    
    public String getIntegrationServerTomcatPath()
    {
		return integrationServerTomcatPath;
	}
    
    
    public ReleaseRequest setIntegrationServerTomcatPath(String integrationServerTomcatPath)
    {
		this.integrationServerTomcatPath = integrationServerTomcatPath;
		return this;
	}
}
