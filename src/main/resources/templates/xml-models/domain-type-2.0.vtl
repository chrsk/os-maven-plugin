<?xml version="1.0" encoding="UTF-8"?>
<domain-type id="$model.getId()" name="$model.getName()"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="$schema">

  <property-set>
    <domain-type-property-set>
    
      #foreach($propertyModel in $model.getPropertyModels(false))
<domain-type-property id="$propertyModel.getId()" name="$propertyModel.getName()" type="$propertyModel.getType()" />
      #end
      
    </domain-type-property-set>
  </property-set>
  
  <primary-key>
  
    #foreach($keyPropertyReferenceModel in $model.getPrimaryKeyModel().getKeyPropertyReferenceModels())
<key property-ref="$keyPropertyReferenceModel.getPropertyReference()" />
    #end
    
  </primary-key>
</domain-type>